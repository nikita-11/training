
public class Person {
	private int age ;
	private String name ;
	//access specifier return_type fun_name(){ } 
	public void display() {
		System.out.println("name:-" + name + "\t" + "Age:-" + age);
	}
	public Person() {
		age= 10 ;
		name = "ALKA" ;
	}
	public Person(int A, String n) {
		age=A ;
		name= n ;
		
	}
	public void setAge(int a) {
		age= a ;
	}
}
