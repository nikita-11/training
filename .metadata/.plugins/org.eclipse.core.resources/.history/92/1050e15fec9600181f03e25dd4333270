
public class demo2 {
	public static void main(String [] args) {
		int [][] arrays ;
		arrays = new int[3][3] ;
		arrays[0][0] = 1 ;
		arrays[0][1] = 12 ;
		arrays[0][2] = 15 ;
		arrays[1][1] =27 ;
		arrays[1][2] =29 ;
		arrays[1][0] =22 ;
		arrays[2][2] = 36 ;
		arrays[2][1] = 39 ;
		arrays[2][0] = 33 ;
		for(int i = 0 ; i< 3;i++) {
			for(int j=0 ; j<3 ;j++) {
				System.out.println(arrays[i][j] + "\t");
			}
			System.out.println();
		}
		
		
	}

}
